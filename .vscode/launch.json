{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "node",
            "request": "launch",
            "program": "lib/server.js",
            "stopOnEntry": false,
            "args": [],
            "cwd": ".",
            "runtimeExecutable": null,
            "runtimeArgs": [
                "--nolazy"
            ],
            "env": {
                "NODE_ENV": "development",
                "PORT": "8080"
            },
            "sourceMaps": true
        },
        {
            "name": "Attach",
            "type": "node",
            "request": "attach",
            "port": 5858
        },
        {
            "name": "Attach to localhost:9229",
            "type": "node",
            "request": "attach",
            "address": "localhost", // <- remote address here
            "port": 9229
          }
    ]
}
