{
  "name": "mirakurun",
  "preferGlobal": true,
  "description": "Japanese DTV Tuner Server Service.",
  "version": "2.6.1",
  "homepage": "https://github.com/Chinachu/Mirakurun",
  "keywords": [
    "mirakurun",
    "chinachu",
    "rivarun",
    "arib",
    "isdb",
    "dvb",
    "dvr",
    "dtv",
    "tv"
  ],
  "author": {
    "name": "Yuki KAN",
    "url": "https://github.com/kanreisa"
  },
  "contributors": [
    "rndomhack"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Chinachu/Mirakurun.git"
  },
  "bugs": {
    "url": "https://github.com/Chinachu/Mirakurun/issues"
  },
  "license": "Apache-2.0",
  "bin": {
    "mirakurun": "bin/cli.sh",
    "mirakurun-epgdump": "bin/epgdump.js"
  },
  "main": "lib/client.js",
  "scripts": {
    "start": "node --max_old_space_size=256 lib/server.js",
    "debug": "node --max_old_space_size=256 --inspect --debug-brk lib/server.js",
    "start.win32": "node.exe --max_old_space_size=256 bin/init.win32.js",
    "debug.win32": "node.exe --max_old_space_size=256 --inspect --debug-brk bin/init.win32.js",
    "build": "gulp build",
    "watch": "gulp watch",
    "test": "gulp test",
    "clean": "gulp clean",
    "prepublish": "gulp build",
    "preinstall": "node bin/preinstall.js",
    "postinstall": "node bin/postinstall.js",
    "preuninstall": "node bin/preuninstall.js"
  },
  "directories": {
    "doc": "doc",
    "lib": "lib"
  },
  "dependencies": {
    "aribts": "1.3.3",
    "body-parser": "^1.17.1",
    "colors": "^1.1.2",
    "express": "^4.15.2",
    "express-openapi": "^1.4.0",
    "ip": "^1.1.4",
    "js-yaml": "^3.8.4",
    "latest-version": "^3.1.0",
    "morgan": "^1.7.0",
    "munin-plugin": "0.0.9",
    "promise-queue": "^2.2.3",
    "semver": "^5.3.0",
    "sift": "^5.0.0",
    "swagger-ui": "2.2.10",
    "tail": "^1.2.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.3",
    "@types/express": "^4.0.35",
    "@types/ip": "0.0.30",
    "@types/js-yaml": "^3.10.1",
    "@types/morgan": "^1.7.35",
    "@types/node": "^8.9.4",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-mocha": "^4.3.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-tslint": "^8.1.3",
    "gulp-typescript": "^4.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  },
  "engines": {
    "node": "^8.9.4 < 9"
  },
  "engineStrict": true,
  "os": [
    "linux",
    "darwin",
    "win32"
  ]
}
