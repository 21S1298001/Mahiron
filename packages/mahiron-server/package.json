{
  "name": "mahiron-server",
  "preferGlobal": true,
  "description": "DVR Tuner Server for Japanese TV.",
  "version": "3.9.0-rc.2",
  "homepage": "https://github.com/21S1298001/Mahiron",
  "license": "Apache-2.0",
  "packageManager": "yarn@3.4.1",
  "main": "dist/client.js",
  "scripts": {
    "start": "ts-node src/server.ts",
    "debug": "ts-node --inspect=0.0.0.0:9229 src/server.ts",
    "start:win32": "node.exe -r source-map-support/register bin/init.win32.js",
    "debug:win32": "node.exe -r source-map-support/register --inspect bin/init.win32.js",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "mocha --exit test/*.spec.js",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "format": "concurrently -n format: -m 1 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write"
  },
  "dependencies": {
    "@chinachu/aribts": "1.3.5-mirakurun.18",
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "eventemitter3": "5.0.0",
    "express": "4.18.2",
    "express-openapi": "12.1.0",
    "ip-num": "1.5.0",
    "js-yaml": "4.1.0",
    "jsonrpc2-ws": "1.0.0-beta24",
    "latest-version": "7.0.0",
    "morgan": "1.10.0",
    "openapi-types": "12.1.0",
    "promise-queue": "2.2.5",
    "rfdc": "1.3.0",
    "semver": "7.3.8",
    "sift": "17.0.1",
    "source-map-support": "0.5.21",
    "swagger-ui-dist": "4.18.1",
    "tail": "2.2.6"
  },
  "devDependencies": {
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/js-yaml": "4.0.5",
    "@types/morgan": "1.9.4",
    "@types/node": "18.15.0",
    "@types/promise-queue": "2.2.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/swagger-ui-dist": "3.30.1",
    "@types/ws": "8.5.4",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "@typescript-eslint/parser": "5.54.1",
    "buffer": "6.0.3",
    "concurrently": "7.6.0",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.7.3",
    "eslint": "8.36.0",
    "eslint-config-prettier": "8.7.0",
    "eslint-plugin-editorconfig": "4.0.2",
    "eslint-plugin-mocha": "10.1.0",
    "mocha": "10.2.0",
    "prettier": "2.8.4",
    "process": "0.11.10",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rimraf": "4.4.0",
    "style-loader": "3.3.1",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "typescript": "4.9.5",
    "url": "0.11.0"
  },
  "engines": {
    "node": ">=18"
  },
  "engineStrict": true,
  "os": [
    "linux",
    "darwin",
    "win32"
  ]
}
