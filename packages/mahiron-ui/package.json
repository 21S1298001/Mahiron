    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "@typescript-eslint/parser": "5.54.1",
    "@vitejs/plugin-react-swc": "3.2.0",
    "concurrently": "7.6.0",
    "eslint": "8.36.0",
    "eslint-config-prettier": "8.7.0",
    "eslint-plugin-editorconfig": "4.0.2",
    "prettier": "2.8.4",
    "typescript": "4.9.5",
    "vite": "4.1.4"
